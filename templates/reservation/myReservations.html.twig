{% extends 'base.html.twig' %}


{% block title %}{{ 'My reservations'|trans }}{% endblock %}

{% block body %}
    <div class="row mt-5 justify-content-center">
        <div class="col-sm-12 box box-success p-3">
            <div class="box-header with-border">
                <div>
                    <span>Esate prisijungęs kaip:</span>
                    <span class="font-weight-bold">{{ username }}</span>
                </div>
                <div>
                    <span>Jūsų el. paštas:</span>
                    <span class="font-weight-bold"> {{ email }}</span>
                </div>
            </div>
            <div class="box-header with-border">
                <div class="h3 text-center done_reservations">Atliktos rezervacijos</div>
            </div>
            <div class="box-body mt-3 mb-3">
                <table class="ur_table mx-auto" style="border: 2px solid black">
                    <tr class="ur_head">
                        <th></th>
                    </tr>
                </table>
            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('build/js/my_reservations.js') }}" defer></script>
    <script>const user_data = '{{ userData|json_encode }}'</script>
{% endblock %}
